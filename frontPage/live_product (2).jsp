      function getNow(n){
        var today=new Date();
        var y=parseInt(today.getFullYear());
        var m=parseInt(today.getMonth())+1;
        var d=parseInt(today.getDate());
        var h=parseInt(today.getHours());
        var mi=parseInt(today.getMinutes());
        m=m<10?'0'+m:m;
        d=d<10?'0'+d:d;
        h=h<10?'0'+h:h;
        
        mi=mi<10?'0'+mi:mi;
        
        if(n==0){
          return y+"-"+m+"-"+d+" "+h+":"+mi;
        }
        else{
          h=parseInt(h)+1;
          return y+"-"+m+"-"+d+" "+h+":"+mi;
        }
      }

